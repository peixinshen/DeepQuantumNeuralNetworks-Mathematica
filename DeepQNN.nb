Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"The", " ", "update", " ", "matrix"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"UpdateMatrix", "[", 
     RowBox[{
     "architecture_", ",", "\[Lambda]_", ",", "\[Epsilon]_", ",", 
      "trainingData_", ",", "currentUnitaries_", ",", "storedStates_", ",", 
      "l_", ",", "j_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"LHS", ",", "RHS", ",", "qubitForm", ",", "layerForm"}], "}"}],
       ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"LHS", "[", "n_", "]"}], ":=", 
        RowBox[{"LayerFeedforward", "[", 
         RowBox[{
         "architecture", ",", "currentUnitaries", ",", "l", ",", "j", ",", 
          RowBox[{"storedStates", "\[LeftDoubleBracket]", 
           RowBox[{"n", ",", 
            RowBox[{"l", "-", "1"}]}], "\[RightDoubleBracket]"}]}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"RHS", "[", "n_", "]"}], ":=", 
        RowBox[{"LayerBackPropagate", "[", 
         RowBox[{
         "architecture", ",", "currentUnitaries", ",", "l", ",", "j", ",", 
          RowBox[{"BackPropagate", "[", 
           RowBox[{"architecture", ",", "currentUnitaries", ",", "l", ",", 
            RowBox[{"trainingData", "\[LeftDoubleBracket]", 
             RowBox[{"n", ",", "2"}], "\[RightDoubleBracket]"}]}], "]"}]}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"the", " ", "exponential", " ", "parameter", " ", "matrix"}], 
        "*)"}], "\[IndentingNewLine]", 
       RowBox[{"qubitForm", "=", 
        RowBox[{"MatrixExp", "[", 
         RowBox[{
          RowBox[{"-", 
           FractionBox["\[Epsilon]", "\[Lambda]"]}], 
          FractionBox[
           SuperscriptBox["2", 
            RowBox[{"architecture", "\[LeftDoubleBracket]", 
             RowBox[{"l", "-", "1"}], "\[RightDoubleBracket]"}]], 
           RowBox[{"Length", "@", "trainingData"}]], " ", 
          RowBox[{"Sum", "[", 
           RowBox[{
            RowBox[{"PartialTrace", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"LHS", "[", "n", "]"}], ".", 
                RowBox[{"RHS", "[", "n", "]"}]}], "-", 
               RowBox[{
                RowBox[{"RHS", "[", "n", "]"}], ".", 
                RowBox[{"LHS", "[", "n", "]"}]}]}], ",", 
              RowBox[{
               RowBox[{"architecture", "\[LeftDoubleBracket]", 
                RowBox[{"l", "-", "1"}], "\[RightDoubleBracket]"}], "+", 
               RowBox[{"Delete", "[", 
                RowBox[{
                 RowBox[{"Range", "@", 
                  RowBox[{
                  "architecture", "\[LeftDoubleBracket]", "l", 
                   "\[RightDoubleBracket]"}]}], ",", "j"}], "]"}]}]}], "]"}], 
            ",", 
            RowBox[{"{", 
             RowBox[{"n", ",", 
              RowBox[{"Length", "@", "trainingData"}]}], "}"}]}], "]"}]}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"layerForm", "=", 
        RowBox[{"LayerUnitary", "[", 
         RowBox[{"architecture", ",", "l", ",", "j", ",", "qubitForm"}], 
         "]"}]}]}]}], "]"}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"The", " ", "cost", " ", "function"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"CostFunction", "[", 
     RowBox[{"trainingData_", ",", "outputStates_"}], "]"}], ":=", 
    RowBox[{"Chop", "@", 
     RowBox[{"Mean", "@", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"trainingData", "\[LeftDoubleBracket]", 
           RowBox[{"i", ",", "2"}], "\[RightDoubleBracket]"}], 
          "\[Conjugate]"}], ".", 
         RowBox[{
         "outputStates", "\[LeftDoubleBracket]", "i", 
          "\[RightDoubleBracket]"}], ".", 
         RowBox[{"trainingData", "\[LeftDoubleBracket]", 
          RowBox[{"i", ",", "2"}], "\[RightDoubleBracket]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "@", "trainingData"}]}], "}"}]}], "]"}]}]}]}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"The", " ", "training", " ", "process"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"QNNTraining", "[", 
     RowBox[{
     "architecture_", ",", "\[Lambda]_", ",", "\[Epsilon]_", ",", 
      "trainingData_", ",", "initialUnitaries_", ",", "steps_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"currentUnitaries", "=", "initialUnitaries"}], ",", 
        "layerWiseOutput", ",", "plotList"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"plotList", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"s", "\[GreaterEqual]", "1"}], ",", 
             RowBox[{"Do", "[", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{
                RowBox[{"currentUnitaries", "\[LeftDoubleBracket]", 
                 RowBox[{"l", ",", "j"}], "\[RightDoubleBracket]"}], "=", 
                RowBox[{
                 RowBox[{"UpdateMatrix", "[", 
                  RowBox[{
                  "architecture", ",", "\[Lambda]", ",", "\[Epsilon]", ",", 
                   "trainingData", ",", "currentUnitaries", ",", 
                   "layerWiseOutput", ",", "l", ",", "j"}], "]"}], ".", 
                 RowBox[{"currentUnitaries", "\[LeftDoubleBracket]", 
                  RowBox[{"l", ",", "j"}], "\[RightDoubleBracket]"}]}]}], ",", 
               RowBox[{"{", 
                RowBox[{"l", ",", "2", ",", 
                 RowBox[{"Length", "@", "architecture"}]}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"j", ",", 
                 RowBox[{
                 "architecture", "\[LeftDoubleBracket]", "l", 
                  "\[RightDoubleBracket]"}]}], "}"}]}], "]"}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"layerWiseOutput", "=", 
            RowBox[{"Feedforward", "[", 
             RowBox[{
             "architecture", ",", "trainingData", ",", "currentUnitaries"}], 
             "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"s", "*", "\[Epsilon]"}], ",", 
             RowBox[{"CostFunction", "[", 
              RowBox[{"trainingData", ",", 
               RowBox[{"layerWiseOutput", "\[LeftDoubleBracket]", 
                RowBox[{"All", ",", 
                 RowBox[{"-", "1"}]}], "\[RightDoubleBracket]"}]}], "]"}]}], 
            "}"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"s", ",", "0", ",", "steps"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"plotList", ",", "currentUnitaries"}], "}"}]}]}], 
     "]"}]}]}]}]], "Input",
